{
	"name": "arima",
	"version": "0.0.1",
	"description": "ðŸŽ¶ A music quiz discord bot designed for lightly competitive fun!",
	"main": "dist/index.js",
	"repository": "https://github.com/Lioness100/Arima",
	"author": "Lioness100 <jchickmm2@gmail.com>",
	"license": "Apache-2.0",
	"type": "module",
	"private": true,
	"imports": {
		"#root/*": "./dist/*.js",
		"#lib/*": "./dist/lib/*.js",
		"#structures/*": "./dist/lib/structures/*.js",
		"#game/*": "./dist/lib/structures/game/*.js",
		"#utils/*": "./dist/lib/utils/*.js",
		"#database/*": "./dist/lib/database/*.js"
	},
	"scripts": {
		"pm2": "pm2 start pm2.ecosystem.yml",
		"start": "node --enable-source-maps .",
		"lint": "eslint src tests --ext js,ts --fix",
		"format": "prettier --write \"{src,tests}/**/*.[jt]s\"",
		"update": "yarn upgrade-interactive",
		"build": "tsup",
		"watch": "tsup --watch",
		"dev": "tsup --watch --onSuccess \"yarn start\"",
		"test": "jest --coverage",
		"typecheck": "tsc -b src",
		"prepare": "husky install"
	},
	"dependencies": {
		"@discordjs/builders": "0.11.0",
		"@mikro-orm/core": "next",
		"@mikro-orm/mongodb": "next",
		"@sapphire/decorators": "^3.1.4",
		"@sapphire/discord.js-utilities": "^4.1.5",
		"@sapphire/framework": "2.3.0",
		"@sapphire/plugin-logger": "^2.1.1",
		"@sapphire/stopwatch": "^1.2.4",
		"@sapphire/time-utilities": "^1.5.0",
		"@sapphire/type": "^2.1.2",
		"@sapphire/utilities": "^3.1.0",
		"@skyra/audio": "^2.0.0",
		"@skyra/jaro-winkler": "^1.0.0",
		"colorette": "^2.0.16",
		"discord-api-types": "^0.26.1",
		"discord.js": "^13.5.1",
		"dotenv": "^10.0.0",
		"envalid": "^7.2.2",
		"fuse.js": "^6.5.3",
		"node-cache": "^5.1.2",
		"spotify-url-info": "^2.2.3",
		"tslib": "^2.3.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.0.2",
		"@commitlint/config-conventional": "^16.0.0",
		"@sapphire/eslint-config": "^4.0.8",
		"@sapphire/prettier-config": "^1.2.7",
		"@sapphire/ts-config": "^3.1.6",
		"@types/jest": "^27.4.0",
		"@types/node": "^16.11.19",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"esbuild-jest": "^0.5.0",
		"eslint": "^8.6.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"jest": "^27.4.7",
		"lint-staged": "^12.1.7",
		"npm-run-all": "^4.1.5",
		"pm2": "^5.1.2",
		"prettier": "^2.5.1",
		"pretty-quick": "^3.1.3",
		"ts-jest": "^27.1.2",
		"tsup": "^5.11.11",
		"typescript": "^4.5.4"
	},
	"engines": {
		"node": ">=16.6",
		"npm": ">=7"
	},
	"lint-staged": {
		"*.{js,ts}": "eslint --fix --ext js,ts"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.1.1"
}
